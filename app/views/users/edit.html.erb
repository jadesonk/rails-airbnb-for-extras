<div class="container-md pt-5">
	<h1>Edit Profile</h1>
		<%= simple_form_for @performer, url: performer_path(@performer) do |f| %>
			<%= f.input :name %>
			<%= f.input :gender %>
			<%= f.input :email %>
			<%= f.input :ethnicity %>
			<%= f.input :age %>
			<%= f.input :location %>
			<%= f.input :physical_attributes %>
			<%= f.input :bio %>
			<%= f.submit %>
		<% end %>
</div>